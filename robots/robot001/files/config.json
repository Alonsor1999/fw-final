{
  "robot_name": "Robot Scraping MercadoLibre",
  "description": "Robot para hacer scraping de productos en MercadoLibre",
  "robot_type": "scraping",
  "scraping_config": {
    "base_url": "https://www.mercadolibre.com.ar",
    "search_term": "laptop",
    "max_pages": 3,
    "timeout": 30,
    "delay_between_requests": 2
  },
  "output_config": {
    "save_to_database": true,
    "save_to_file": true,
    "file_path": "output/products.json"
  }
}
